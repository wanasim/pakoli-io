{
  "extends": "tsconfig/nextjs.json",
  "include": [
    ".",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../packages/pakolicons/NavLogo.tsx"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["./components/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
